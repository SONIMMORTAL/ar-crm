{
    "cron": [
        {
            "path": "/api/sync/mailchimp/audience",
            "schedule": "0 0 * * *"
        },
        {
            "path": "/api/sync/nationbuilder/contact",
            "schedule": "0 1 * * *"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                }
            ]
        }
    ]
}